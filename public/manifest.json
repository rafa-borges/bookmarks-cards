{
  "manifest_version": 3,
  "name": "Bookmarks Cards",
  "version": "2.0.0",

  "chrome_url_overrides" : {
    "newtab": "index.html"
  },

  "permissions": ["bookmarks", "sessions", "tabs", "favicon"],

  "web_accessible_resources": [{
    "resources": ["_favicon/*"],
    "matches": ["<all_urls>"]
  }]

}